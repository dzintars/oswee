{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": [
        "apps/front/src/store/src/types/*"
      ],
      "oswee/*": [
        "*"
      ]
    },
    "module": "CommonJS",
    "noImplicitAny": false,
    "noImplicitThis": true,
    "preserveConstEnums": true,
    "removeComments": true,
    "target": "ES6",
    "sourceMap": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "lib": [
      "ES2018",
      "DOM",
      "DOM.Iterable"
    ]
  },
  "exclude": [
    "bazel-*",
    "node_modules"
  ]
}
