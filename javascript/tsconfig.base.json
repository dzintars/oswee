{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "rootDirs": [
      ".",
      "../bazel-out/host/libraries/a",
      "../bazel-out/darwin-fastbuild/bin/libraries/a",
      "../bazel-out/k8-fastbuild/bin/libraries/a",
      "../bazel-out/x64_windows-fastbuild/bin/libraries/a",
      "../bazel-out/darwin-dbg/bin/libraries/a",
      "../bazel-out/k8-dbg/bin/libraries/a",
      "../bazel-out/x64_windows-dbg/bin/libraries/a"
    ],
    "module": "CommonJS",
    "target": "ES2019",
    "lib": ["ES2019", "DOM", "DOM.iterable"],
    "esModuleInterop": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "importHelpers": true,
    "preserveConstEnums": true,
    "removeComments": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "typeRoots": ["node_modules/@types"],
    "paths": {
      "@oswee/*": ["*", "bazel-bin/*", "bazel-genfiles/*"],
      "@oswee/modules/*": ["javascript/modules/*/src"],
      "@oswee/packages/*": ["javascript/packages/*/src"],
      "@oswee/tools/*": ["tools/*/src"],
      "@oswee/platform/web/assets/*": ["platform/web/src/assets/*"],
      "@oswee/container/*": ["platform/web/src/ui/containers/*/src"],
      "@oswee/feature/*": ["platform/web/src/store/src/lib/features/*/src"],
      "@oswee/proto/*": ["bazel-bin/proto/oswee/*"]
    },
    "plugins": [
      {
        "alwaysShowRuleFailuresAsWarnings": false,
        "ignoreDefinitionFiles": true,
        "disableNoUnusedVariableRule": false,
        "suppressWhileTypeErrorsPresent": false
      }
    ]
  },
  "exclude": ["bazel-*", "node_modules"]
}
