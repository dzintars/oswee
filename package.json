    "swagger-ui": "^3.52.0",
    "tmp": "^0.2.1",
    "tslib": "^2.3.0",
    "typescript": "^4.0.3",
    "uglify-js": "^3.14.1",
    "umd-compat-loader": "^2.1.2",
    "webpack": "^5.42.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0",
    "xmlbuilder": "^15.1.1",
    "yaml-loader": "^0.6.0",
