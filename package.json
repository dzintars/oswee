    "chalk": "^4.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "commitlint": "^12.1.4",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "csstype": "^3.0.8",
    "escodegen": "^2.0.0",
    "eslint": "^7.30.0",
    "espree": "^8.0.0",
    "estraverse": "^5.2.0",
    "fs": "^0.0.1-security",
    "glob": "^7.1.7",
    "google-protobuf": "^3.17.3",
    "html-webpack-plugin": "^5.3.2",
    "http-server": "^0.12.3",
    "husky": "^7.0.1",
    "jsdoc": "^3.6.7",
    "json-loader": "^0.5.7",
    "lint-staged": "^11.0.0",
    "minimist": "^1.2.5",
